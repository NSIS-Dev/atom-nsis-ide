{
  "name": "nsis-ide",
  "version": "0.18.0",
  "description": "Adds IDE-like features for NSIS development in Atom",
  "license": "MIT OR GPL-2.0",
  "main": "./lib/nsis-ide",
  "repository": {
    "type": "git",
    "url": "https://github.com/NSIS-Dev/atom-nsis-ide.git"
  },
  "keywords": [
    "nsis",
    "nullsoft",
    "installer",
    "setup",
    "windows",
    "build",
    "build tool",
    "compiler",
    "grammar",
    "language syntax",
    "ide"
  ],
  "engines": {
    "atom": ">=1.12.0 <2.0.0"
  },
  "package-deps": [
    "browse",
    "build",
    "build-makensis",
    "build-makensis-wine",
    "build-nsl",
    "console-panel",
    "edit-in-new-tab",
    "logo-file-icons",
    "haskell-nsis",
    "hyperclick-nsis",
    "language-ini",
    "language-nsis",
    "language-nsl",
    "linter:2.0.0",
    "linter-ui-default",
    "linter-makensis",
    "minimap",
    "minimap-linter",
    "minimap-selection",
    "nsis-plugins",
    "tool-bar",
    "set-syntax"
  ],
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "dependencies": {
    "atom-package-deps": "^4.6.2",
    "getmac": "^1.4.1"
  },
  "devDependencies": {
    "coffeelint-stylish": "^0.1.2",
    "gulp": "^4.0.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^4.0.0",
    "gulp-jsonlint": "^1.2.1",
    "husky": "^0.14.3"
  },
  "scripts": {
    "precommit": "npm run test",
    "test": "gulp lint"
  }
}
