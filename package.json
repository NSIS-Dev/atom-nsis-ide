{
  "name": "nsis-ide",
  "version": "0.18.2",
  "description": "Adds IDE-like features for NSIS development in Atom",
  "license": "MIT OR GPL-2.0",
  "main": "./lib/nsis-ide",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode none --watch",
    "lint": "gulp lint",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NSIS-Dev/atom-nsis-ide.git"
  },
  "keywords": [
    "nsis",
    "nullsoft",
    "installer",
    "setup",
    "windows",
    "build",
    "build tool",
    "compiler",
    "grammar",
    "language syntax",
    "ide"
  ],
  "engines": {
    "atom": ">=1.12.0 <2.0.0"
  },
  "package-deps": [
    "browse",
    "build",
    "build-makensis",
    "build-makensis-wine",
    "build-nsl",
    "console-panel",
    "edit-in-new-tab",
    "logo-file-icons",
    "haskell-nsis",
    "hyperclick",
    "hyperclick-nsis",
    "language-ini",
    "language-nsis",
    "language-nsl",
    "linter:2.0.0",
    "linter-ui-default",
    "linter-makensis",
    "minimap",
    "minimap-linter",
    "minimap-selection",
    "nsis-plugins",
    "tool-bar",
    "set-syntax"
  ],
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "dependencies": {
    "atom-package-deps": "^5.1.0"
  },
  "devDependencies": {
    "coffee-loader": "^0.9.0",
    "coffeelint-stylish": "^0.1.2",
    "coffeescript": "^2.4.1",
    "gulp": "^4.0.2",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^4.0.0",
    "gulp-jsonlint": "^1.3.0",
    "husky": "^2.3.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "apmInstallSource": {
    "type": "git",
    "source": "https://github.com/NSIS-Dev/atom-nsis-ide",
    "sha": "009987c78761799b2db901d971e417ac9a868420"
  }
}
