{
  "name": "nsis-ide",
  "version": "0.20.0",
  "description": "Adds IDE-like features for NSIS development in Atom",
  "license": "MIT OR GPL-2.0",
  "main": "./lib/nsis-ide",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode none --watch",
    "lint": "gulp lint",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NSIS-Dev/atom-nsis-ide.git"
  },
  "keywords": [
    "nsis",
    "nullsoft",
    "installer",
    "setup",
    "windows",
    "build",
    "build tool",
    "compiler",
    "grammar",
    "language syntax",
    "ide"
  ],
  "engines": {
    "atom": ">=1.12.0 <2.0.0"
  },
  "package-deps": [
    {
      "name": "browse",
      "minimumVersion": "3.0.0"
    },
    {
      "name": "buildium"
    },
    {
      "name": "build-makensis"
    },
    {
      "name": "build-makensis-wine"
    },
    {
      "name": "build-nsl"
    },
    {
      "name": "console-panel"
    },
    {
      "name": "edit-in-new-tab"
    },
    {
      "name": "logo-file-icons"
    },
    {
      "name": "haskell-nsis"
    },
    {
      "name": "hyperclick"
    },
    {
      "name": "hyperclick-nsis"
    },
    {
      "name": "language-ini"
    },
    {
      "name": "language-nsis",
      "minimumVersion": "8.0.0"
    },
    {
      "name": "language-nsl"
    },
    {
      "name": "linter:2.0.0"
    },
    {
      "name": "linter-ui-default"
    },
    {
      "name": "linter-makensis"
    },
    {
      "name": "minimap"
    },
    {
      "name": "minimap-linter"
    },
    {
      "name": "minimap-selection"
    },
    {
      "name": "nsis-plugins"
    },
    {
      "name": "tool-bar"
    },
    {
      "name": "set-syntax"
    }
  ],
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "dependencies": {
    "atom-package-deps": "^7.2.2"
  },
  "devDependencies": {
    "coffee-loader": "^2.0.0",
    "coffeelint-stylish": "^0.1.2",
    "coffeescript": "^2.4.1",
    "gulp": "^4.0.2",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^4.0.0",
    "gulp-jsonlint": "^1.3.0",
    "husky": "^4.3.0",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "apmInstallSource": {
    "type": "git",
    "source": "https://github.com/NSIS-Dev/atom-nsis-ide",
    "sha": "009987c78761799b2db901d971e417ac9a868420"
  }
}
