{
  "name": "nsis-ide",
  "version": "0.7.0",
  "description": "Adds IDE-like features for NSIS development in Atom",
  "license": "MIT OR GPL-2.0",
  "main": "./lib/nsis-ide",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NSIS-Dev/atom-nsis-ide.git"
  },
  "keywords": [
    "nsis",
    "nullsoft",
    "installer",
    "setup",
    "windows",
    "build",
    "build tool",
    "compiler",
    "grammar",
    "language syntax",
    "ide"
  ],
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "package-deps": [
    "browse",
    "build",
    "build-makensis",
    "build-makensis-wine",
    "build-nsl",
    "file-icons",
    "keyboard-localization",
    "language-ini",
    "language-nlf",
    "language-nsis",
    "language-nsl",
    "linter",
    "minimap",
    "minimap-highlight-selected",
    "minimap-linter",
    "minimap-selection",
    "nsis-plugins",
    "tool-bar",
    "set-syntax"
  ],
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "dependencies": {
    "atom-package-deps": "^4.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "coffeelint-stylish": "^0.1.2",
    "eslint": "^3.7.1",
    "eslint-config-atom-build": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^2.1.2",
    "gulp-jshint": "^2.0.1",
    "gulp-jsonlint": "^1.1.2",
    "gulp-lesshint": "^2.0.0",
    "jshint": "^2.9.3"
  },
  "scripts": {
    "test": "gulp lint"
  }
}
